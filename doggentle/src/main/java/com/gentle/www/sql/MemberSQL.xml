<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DYD Mapper 3.0//EN"
			"http://mybatis.org/dyd/mybatis-3-mapper.dtd">
<mapper namespace="mSQL">
	<select id="login" resultType="int" parameterType="mVO">
		SELECT
			count(*)
		FROM
			member
		WHERE
			id = #{id}
			AND pw = #{pw}
			AND isshow = 'cst'
	</select>
</mapper>